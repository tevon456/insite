{
  "name": "garnet",
  "version": "1.1.10",
  "description": "React based Dashboard that uses infoset-ng as data store",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest -u",
    "test-dev": "NODE_ENV=test jest -u --watch",
    "start": "NODE_ENV=development node server.js",
    "build": "NODE_ENV=production webpack --config webpack/common.config.js --progress --colors",
    "dev": "webpack-dev-server --content-base www/static --inline --hot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PalisadoesFoundation/garnet"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PalisadoesFoundation/garnet/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/.transform/preprocessor.js",
      "^.+\\.less$": "<rootDir>/.transform/lessTransform.js",
      "^.+\\.png$": "<rootDir>/.transform/pngTransform.js"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      ".*\\.less$": "<rootDir>/.transform/lessTransformer.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "homepage": "https://github.com/PalisadoesFoundation/garnet",
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap": "^3.3.7",
    "d3": "^4.4.1",
    "d3-scale": "^1.0.6",
    "d3-time": "^1.0.7",
    "grid-styled": "^2.0.0-10",
    "jquery": "^3.1.1",
    "less": "^2.7.1",
    "moment": "^2.17.1",
    "morgan": "^1.8.2",
    "react": "^15.6.1",
    "react-bootstrap": "^0.30.10",
    "react-dom": "^15.6.1",
    "react-router": "^3.0.1",
    "react-redux": "^5.0.7 ",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk":"2.2.0",
    "recharts": "^0.20.2",
    "rxjs": "^5.4.2",
    "strman": "^2.0.0",
    "yamljs": "^0.2.10"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.29.0",
    "imports-loader": "^0.7.0",
    "jest": "^20.0.4",
    "jest-cli": "^19.0.2",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react-addons-test-utils": "^15.4.2",
    "react-loadable": "^4.0.4",
    "react-test-renderer": "^15.4.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  }
}
